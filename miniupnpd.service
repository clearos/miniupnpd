[Unit]
Description=UPnP device server.
Documentation=man:miniupnpd(8)
After=network.target

[Service]
Type=forking
EnvironmentFile=-/etc/sysconfig/miniupnpd
ExecStart=/usr/sbin/miniupnpd \
 -f /etc/miniupnpd/miniupnpd.conf \
 $MINIUPNPD_WAN $MINIUPNPD_LANS $MINIUPNPD_URL

Restart=always
RestartSec=1
TimeoutSec=5

[Install]
WantedBy=multi-user.target
